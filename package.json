{
   "name": "toypack-vue",
   "description": "Vue plugin for Toypack.",
   "version": "0.0.0-development",
   "type": "module",
   "main": "./build/index.js",
   "types": "./build/index.d.ts",
   "license": "MIT",
   "publishConfig": {
      "access": "public"
   },
   "scripts": {
      "dev": "concurrently \"set NODE_ENV=development\" \"npm run build:main -- --watch\" \"npm run type:dts -- --watch\"",
      "build": "concurrently \"set NODE_ENV=production\" \"npm run clean\" \"npm run build:main\" \"npm run build:bundle\" \"npm run type:dts\"",
      "build:main": "swc ./src -d ./build",
      "build:bundle": "webpack",
      "clean": "rimraf build browser coverage",
      "type:dts": "tsc --emitDeclarationOnly",
      "type:check": "tsc --noEmit",
      "format": "prettier \"src/**/*.ts\" --write",
      "format:check": "prettier \"src/**/*.ts\" --check",
      "lint": "eslint src --ext .ts --fix",
      "lint:check": "eslint src --ext .ts",
      "test": "vitest run",
      "test:watch": "vitest watch",
      "test:coverage": "vitest run --coverage",
      "cz": "cz"
   },
   "devDependencies": {
      "@ryansonshine/commitizen": "^4.2.8",
      "@ryansonshine/cz-conventional-changelog": "^3.3.4",
      "@swc/cli": "^0.1.62",
      "@swc/core": "^1.3.52",
      "@typescript-eslint/eslint-plugin": "^5.59.0",
      "@typescript-eslint/parser": "^5.59.0",
      "@vitest/coverage-v8": "^0.32.4",
      "concurrently": "^8.0.1",
      "eslint": "^8.38.0",
      "eslint-config-prettier": "^8.8.0",
      "eslint-import-resolver-typescript": "^3.5.5",
      "eslint-plugin-eslint-comments": "^3.2.0",
      "eslint-plugin-import": "^2.27.5",
      "jsdom": "^21.1.1",
      "prettier": "^2.8.7",
      "rimraf": "^4.4.1",
      "ts-loader": "^9.4.3",
      "typescript": "^5.1.3",
      "vitest": "^0.33.0",
      "webpack": "^5.86.0",
      "webpack-cli": "^5.1.4"
   },
   "files": [
      "./build",
      "./browser"
   ],
   "keywords": []
}
